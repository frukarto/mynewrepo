			<xs:maxInclusive value="100.0" fixed="false"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LandlinePhoneType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InstallAMPM">
		<xs:annotation>
			<xs:documentation>Represents Installation/Service is requested in AM or PM</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ValidateType">
		<xs:annotation>
			<xs:documentation>Indicates whether the validation flag is true or false
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:simpleType name="OrderedSiteType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="InstallationDesiredDateTimeType">
