<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.hns.com/iag/schemas/core" targetNamespace="http://www.hns.com/iag/schemas/core" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="CastorableException">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="parameter" minOccurs="0" maxOccurs="unbounded">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="error_code" type="xs:int" use="required"/>
			<xs:attribute name="message" type="xs:string" use="required"/>
			<xs:attribute name="core_exception_subclass_name" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="OperationRollbackMsg">
		<xs:annotation>
			<xs:documentation>Rollback Message to invoke a rollback operation.  This message is intended primarily for use with non-transactional systems/componenets that need to provide rollback data to the caller.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RollbackData" type="RollbackDataType"/>
				<xs:element name="UserCredential" type="UserCredentialType" minOccurs="0"/>
				<xs:element name="ErrorCode" type="ErrorCodeType" minOccurs="0"/>
				<xs:element name="ErrorMessage" type="ErrorMessageType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="RollbackDataType">
		<xs:annotation>
			<xs:documentation>Rollback data that will be sent in case of error during ordering.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="UserCredentialType">
		<xs:annotation>
			<xs:documentation>
                This is a complex type for User Credential, a corresponding
                UserCredential Bean exists in framework
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LoginId" type="UserIDType"/>
			<xs:element name="SessionKey" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Roles" minOccurs="0" maxOccurs="unbounded">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SiteCodes" minOccurs="0" maxOccurs="unbounded">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AuthorizationFlag" type="xs:boolean"/>
			<xs:element name="ProtocolId" type="ProtocolIdType" minOccurs="0"/>
			<xs:element name="CaseId" type="CaseIdType" minOccurs="0"/>
			<xs:element name="Customer_Login" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="UserIDType">
		<xs:annotation>
			<xs:documentation>Stores the value of Site-owner for Direcway consumers. This value is provided by SM at the time of Site Creation for Consumers</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="25"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="StandardResponseMsg">
		<xs:annotation>
			<xs:documentation>SRM: Standard Response Message used as response message for CRM CRMI messages</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageHeader" type="CommonMessageHeaderType"/>
				<xs:element name="ResponseStatus" type="CommonResponseStatusType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Common Message Header Type -->
	<xs:complexType name="CommonMessageHeaderType">
		<xs:annotation>
			<xs:documentation>Common Application Header type used in all CRM, ACS, and VISION messages</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionDateTime" type="TransactionDateTimeType"/>
			<xs:element name="TransactionCode" type="TransactionCodeType"/>
			<xs:element name="TransactionSequenceId" type="TransactionSequenceIdType"/>
			<xs:element name="MessageSource" type="MessageSourceType" minOccurs="0"/>
			<xs:element name="MessageDestination" type="MessageSourceType" minOccurs="0"/>
			<xs:element name="OperatorName" type="OperatorNameType" minOccurs="0"/>
			<xs:element name="ProtocolNumber" type="ProtocolIdType" minOccurs="0"/>
			<xs:element name="CaseId" type="CaseIdType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="TransactionDateTimeType">
		<xs:annotation>
			<xs:documentation>Transaction date and time string. This is of the format: mm/dd/yyyy hh:mm:ss . </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="19"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Transaction Code. (10 letters)-->
	<xs:simpleType name="TransactionCodeType">
		<xs:annotation>
			<xs:documentation>10 Letter Transaction Code used in the messages Possible Values : ORDER</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TransactionSequenceIdType">
		<xs:annotation>
			<xs:documentation>When generated by WebACS it is D000000000 i.e D followed by 9 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MessageSourceType">
		<xs:annotation>
			<xs:documentation>Message Source (or Destination) used in the common message header: DSS, DCCS, WEBACS, CRM, or VISION are example values. This value should be the SDS Sub-system ID from the SDS_Subsystems table of the Common User Database</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperatorNameType">
		<xs:annotation>
			<xs:documentation>Operator Name Type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Common Response Status  -->
	<xs:complexType name="CommonResponseStatusType">
		<xs:annotation>
			<xs:documentation>Common Response Status Type</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Status" type="xs:boolean"/>
			<xs:element name="ResponseCode" type="ResponseCodeType" minOccurs="0"/>
			<xs:element name="ResponseMessage" type="ResponseMessageType" minOccurs="0"/>
			<xs:element name="RollbackData" type="RollbackDataType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ResponseCodeType">
		<xs:annotation>
			<xs:documentation>Response Code ( 10 character max) If provided this code should not be an empty string.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
			<xs:pattern value="([A-Za-z0-9])*(:|\-|_| |#)?([A-Za-z0-9])*"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Response Message -->
	<xs:simpleType name="ResponseMessageType">
		<xs:annotation>
			<xs:documentation>This provides a brief response message ( 256 characters max length)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="256"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BasicObjType">
		<xs:sequence>
			<xs:element name="Prop" type="PropType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContainerObjType">
		<xs:sequence>
			<xs:element name="BasicObj" type="BasicObjType" minOccurs="0"/>
			<xs:element name="TableObj" type="TableObjType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Type" type="xs:string" use="optional" default="MMVInterface"/>
		<xs:attribute name="Name" type="xs:string" use="optional" default="SiteId"/>
		<xs:attribute name="Nmd" type="xs:string" use="optional" default="Nmd"/>
	</xs:complexType>
	<xs:complexType name="PropType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="Name" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="RowType">
		<xs:sequence>
			<xs:element name="Prop" type="PropType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="IndexName" type="xs:string" use="required"/>
		<xs:attribute name="IndexValue" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="TableObjType">
		<xs:sequence>
			<xs:element name="Row" type="RowType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Type" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:simpleType name="EnterpriseNameType">
		<xs:annotation>
			<xs:documentation>Indicates name of the Enterprise.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="UpfrontChargeTransIdType">
		<xs:annotation>
			<xs:documentation>Unique Id to identify an upfront charge payment</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SANType">
		<xs:annotation>
			<xs:documentation> Site Account Number used to uniquely identify a site or appliance in DSS.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="24"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="EnterpriseShortType">
		<xs:sequence>
			<xs:element name="EnterpriseFullName" type="EnterpriseNameType" minOccurs="0"/>
			<xs:element name="EnterpriseId" type="CustomerIDType"/>
			<xs:element name="EnterpriseName" type="EnterpriseNameType"/>
			<xs:element name="MAN" type="SANType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EnterpriseType">
		<xs:sequence>
			<xs:element name="EnterpriseFullName" type="EnterpriseNameType" minOccurs="0"/>
			<xs:element name="EnterpriseId" type="CustomerIDType"/>
			<xs:element name="EnterpriseName" type="EnterpriseNameType"/>
			<xs:element name="MaxEnterpUsers" type="xs:int" default="-1" minOccurs="0"/>
			<xs:element name="UppURL" type="URLType" minOccurs="0"/>
			<xs:element name="AppURL" type="URLType" minOccurs="0"/>
			<xs:element name="MAN" type="SANType" minOccurs="0"/>
			<xs:element name="OutletFlag" type="xs:int" minOccurs="0"/>
			<xs:element name="WebAccessKey" type="WebAccessKeyType" minOccurs="0"/>
			<xs:element name="MaxTimeout" type="xs:int" default="-1" minOccurs="0"/>
			<xs:element name="IdleTimeout" type="xs:int" default="-1" minOccurs="0"/>
			<xs:element name="PasswordExpiryDays" type="xs:int" default="-1" minOccurs="0"/>
			<xs:element name="EnterpriseAddress" type="EnterpriseAddressType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="URLType">
		<xs:annotation>
			<xs:documentation>The location on the Web. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="WebAccessKeyType">
		<xs:annotation>
			<xs:documentation>A key that is used to access the customer's program Web page.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="8"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="EnterpriseAddressType">
		<xs:sequence>
			<xs:element name="CompanyName" type="CompanyNameType"/>
			<xs:element name="EnterpriseName" type="EnterpriseNameType" minOccurs="0"/>
			<xs:element name="FirstName" type="FirstNameType" minOccurs="0"/>
			<xs:element name="LastName" type="LastNameType" minOccurs="0"/>
			<xs:element name="StreetAddress" type="AddressType" minOccurs="0"/>
			<xs:element name="FaxNumber" type="PhoneType" minOccurs="0"/>
			<xs:element name="PhoneNumber" type="PhoneType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CompanyNameType">
		<xs:annotation>
			<xs:documentation>Indicates name of the Company.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="90"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CustomerCompanyNameType">
		<xs:annotation>
			<xs:documentation>This type is similar to CompanyNameType (which is used in customer management flows). This type is used in ordering flows.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="VoidResponseMsg">
		<xs:annotation>
			<xs:documentation>
                This message provides a semantically 'void' response
                for synchronous messages that do not need to return a
                specific response or other payload and do not need to
                or can not provide rollback data.
                This is a dummy type that simply provides the base type for the VoidResponseMsg, ErrorResponseMsg, and    					GenericRollbackResponseMsg messages
            </xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:element name="GenericRollbackResponseMsg">
		<xs:annotation>
			<xs:documentation>
                This message should/may be used in response to a
                create/update/delete message in order to return a
                RollbackData string
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="VoidResponseMsg">
					<xs:sequence>
						<xs:element name="RollbackData" type="RollbackDataType"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ErrorResponseMsg">
		<xs:annotation>
			<xs:documentation>
                This message should/may be used in response to any
                message in order to return an error response. This
                is an alternative approach to simply throwing an
                exception which provides greater portability, and
                reduces coupling between systems by not requiring
                access to or knowledge of exception classes belonging
                to other entities.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="VoidResponseMsg">
					<xs:sequence>
						<xs:element name="ErrorCode" type="ErrorCodeType" minOccurs="0"/>
						<xs:element name="ErrorMessage" type="ErrorMessageType" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<!--  String Vector  -->
	<xs:complexType name="StringSequenceType">
		<xs:annotation>
			<xs:documentation>Vector of Strings</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StringValue" type="EmailISPAccountNameType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="EmailISPAccountNameType">
		<xs:annotation>
			<xs:documentation>32 Character String</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="32"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CRMServiceOfferingType">
		<xs:annotation>
			<xs:documentation>Service Offering List Type
			Parameter Tag for each service offering derived using billing service id to tag mapping. Possible values are:
				1.Hardware
				2.Install
				3.Transport
				4.Warranty
				5.Email
				6.Premium Call Center Support
				5.Others
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ServiceOfferingId" type="SrvOffIdType"/>
			<xs:element name="ServiceOfferingName" type="SrvOffNameType"/>
			<xs:element name="ServiceOfferingDesc" type="SrvOffDescType"/>
			<xs:element name="Tag" type="xs:string" minOccurs="0"/>
			<xs:element name="ServiceSpecs" type="ServiceSpecsType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Sevice Specs Type-->
	<xs:complexType name="ServiceSpecsType">
		<xs:annotation>
			<xs:documentation>
				     For Ju SpecId would be NMS Service Code ID, SpecName would be NMS Service Code Name and Value would be NMS Service Code Value.
				     For Ka, Ku, SM Dummy, PPS all of these parameters are not supported
				     For CRMIS SpecId would be Product Id/BOM and SpecName is the description of Product Id/BOM, Value - NA
				</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SpecId" type="xs:string" minOccurs="0"/>
			<xs:element name="SpecName" type="xs:string" minOccurs="0"/>
			<xs:element name="Value" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Sevice Offerings Id Type-->
	<xs:simpleType name="SrvOffIdType">
		<xs:annotation>
			<xs:documentation>Service Offering ID</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:maxInclusive value="9999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- Sevice Offerings Name Type-->
	<xs:simpleType name="SrvOffNameType">
		<xs:annotation>
			<xs:documentation>Service Offering Name (100 characters max length)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SrvOffDescType">
		<xs:annotation>
			<xs:documentation>Brief Service Offering Description (4000 characters max length)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="4000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PurchaseOrderNumberType">
		<xs:annotation>
			<xs:documentation>The Purchase Order Number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ServiceDealListType">
		<xs:annotation>
			<xs:documentation>A collection of service deals with TNM information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DealName" type="DealNameType"/>
			<xs:element name="OrderStatus" type="OrderStatusType"/>
			<xs:element name="Quantity">
				<xs:simpleType>
					<xs:restriction base="xs:double">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Cost" type="xs:double" minOccurs="0"/>
			<xs:element name="BENType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AuthorizationNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CRMIS shall populate it from T_SRCREFERENCE field of BEN</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="InvoiceDesc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CRMIS shall populate it from PDT table</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="InstallerNotes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Installer notes describing about installation.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4000"/>
					</xs:restriction>
				</xs:simpleType>
