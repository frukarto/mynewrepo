			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AddressValueType">
		<xs:sequence>
			<xs:element name="BarCodeDigits" type="xs:string" minOccurs="0"/>
			<xs:element name="DPVCode" type="xs:string" minOccurs="0"/>
			<xs:element name="DPVDescription" type="xs:string" minOccurs="0"/>
			<xs:element name="DPVNotes" type="xs:string" minOccurs="0"/>
			<xs:element name="DPVNotesDescription" type="xs:string" minOccurs="0"/>
			<xs:element name="CorrectionCode" type="xs:string" minOccurs="0"/>
			<xs:element name="CorrectionDescription" type="xs:string" minOccurs="0"/>
			<xs:element name="EACFValidationStatus" type="xs:int" minOccurs="0"/>
			<xs:element name="EACFValidationStatusMessage" type="xs:string" minOccurs="0"/>
			<xs:element name="EACFErrorCode" type="xs:int" minOccurs="0"/>
			<xs:element name="EACFErrorMessage" type="xs:string" minOccurs="0"/>
			<xs:element name="FullAddressCorrectedFlag" type="xs:boolean" minOccurs="0"/>
			<xs:element name="PartialAddressCorrectedFlag" type="xs:boolean" minOccurs="0"/>
			<xs:element name="AddressValidatedFlag" type="xs:boolean" minOccurs="0"/>
			<xs:element name="Status" type="xs:boolean" minOccurs="0"/>
			<xs:element name="ErrorCode" type="xs:int" minOccurs="0"/>
			<xs:element name="ErrorMessage" type="xs:string" minOccurs="0"/>
			<xs:element name="GeoAddress" type="cct:GeoAddressType" minOccurs="0"/>
			<xs:element name="CorrectedAddress" type="cct:BasicAddressType"/>
			<xs:element name="IsForcedAddress" type="xs:boolean" minOccurs="0"/>
			<xs:element name="AddressType" type="xs:string" minOccurs="0"/>
