				<xs:annotation>
					<xs:documentation>RBDI stands for Residential and Business Delivery Indicator. Possible Values are Residential, Business, POBox, Unknown.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GeoAddressType">
		<xs:sequence>
			<xs:element name="Latitude" type="xs:float"/>
			<xs:element name="Longitude" type="xs:float"/>
